        "mocha": "^10.2.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.0"
    },
    "scripts": {
        "build": "tsc",
        "lint": "eslint --max-warnings=0 -c .eslintrc.js --ext .ts .",
